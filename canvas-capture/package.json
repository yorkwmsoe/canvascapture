{
    "name": "@canvas-capture/lib",
    "private": true,
    "version": "0.0.0",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "package": "tsup",
        "lint": "eslint . --max-warnings=0",
        "lint:fix": "eslint . --fix",
        "test": "vitest",
        "test:cli": "vitest run",
        "test:ui": "vitest --ui",
        "test:mutate": "stryker run",
        "coverage": "vitest run --coverage",
        "typeorm": "typeorm-ts-node-esm",
        "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./typeorm.config.ts",
        "typeorm:generate-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate ./migrations/$npm_config_name",
        "typeorm:create-migration": "npm run typeorm -- migration:create ./src/migration/$npm_config_name",
        "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert"
    },
    "devDependencies": {
        "@stryker-mutator/typescript-checker": "^8.7.1",
        "@stryker-mutator/vitest-runner": "^8.7.1",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^7.0.1",
        "@vitest/coverage-v8": "^1.2.1",
        "@vitest/ui": "^1.2.2",
        "eslint": "^8.56.0",
        "msw": "^2.2.14",
        "ts-node": "^10.9.2",
        "typescript": "^5.2.2",
        "vitest": "^1.2.1"
    },
    "dependencies": {
        "@types/node": "^20.11.19",
        "axios": "^1.6.7",
        "date-fns": "^3.3.1",
        "reflect-metadata": "^0.2.2",
        "sqlite3": "^5.1.7",
        "tsup": "^8.0.2",
        "typeorm": "^0.3.22"
    }
}
