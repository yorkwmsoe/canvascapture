{
    "name": "canvas-capture-cli",
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "type": "module",
    "private": true,
    "bin": {
        "canvascapture": "./build/cli.js"
    },
    "scripts": {
        "test": "vitest",
        "test:ui": "vitest --ui",
        "coverage": "vitest run --coverage",
        "start": "vite-node src/cli.ts",
        "build": "vite build",
        "lint": "eslint . --max-warnings=0",
        "lint:fix": "eslint . --fix",
        "package:win-x64": "pkg . --targets node18-win-x64 --output release/canvascapture-win-x64",
        "package:win-arm64": "pkg . --targets node18-win-arm64 --output release/canvascapture-win-arm64",
        "package:linux-x64": "pkg . --targets node18-linux-x64 --output release/canvascapture-linux-x64",
        "package:linux-arm64": "pkg . --targets node18-linux-arm64 --output release/canvascapture-linux-arm64",
        "package:macos-x64": "pkg . --targets node18-macos-x64 --output release/canvascapture-macos-x64",
        "package:macos-arm64": "pkg . --targets node18-macos-arm64 --output release/canvascapture-macos-arm64"
    },
    "pkg": {
        "scripts": "build/**/*.js",
        "assets": "node_modules/figlet/fonts/Standard.flf"
    },
    "keywords": [],
    "author": "MSOE",
    "license": "UNLICENSED",
    "devDependencies": {
        "@liuli-util/vite-plugin-node": "^0.8.1",
        "@types/figlet": "^1.5.8",
        "@types/gradient-string": "^1.1.5",
        "@types/inquirer": "^9.0.7",
        "@types/lodash": "^4.14.202",
        "@types/markdown-it": "^13.0.7",
        "@types/node": "^20.11.20",
        "@types/prompt-sync": "^4.2.3",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitest/coverage-v8": "^1.3.1",
        "@vitest/ui": "^1.3.1",
        "eslint": "^8.57.0",
        "pkg": "^5.8.1",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.8",
        "tsx": "^3.14.0",
        "typescript": "^5.3.3",
        "vite": "^5.0.11",
        "vite-node": "^1.3.1",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "^1.3.1"
    },
    "dependencies": {
        "@canvas-capture/lib": "workspace:*",
        "@inquirer/prompts": "^3.3.2",
        "@inquirer/testing": "^2.1.11",
        "axios": "^1.6.7",
        "date-fns": "^2.30.0",
        "figlet": "^1.7.0",
        "gradient-string": "^2.0.2",
        "lodash": "^4.17.21",
        "markdown-it": "^14.0.0",
        "pino": "^8.19.0",
        "prompt-sync": "^4.2.0",
        "pyodide": "^0.25.0",
        "zod": "^3.22.4"
    }
}
