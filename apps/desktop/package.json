{
  "name": "canvas-capture-desktop",
  "version": "0.0.1",
  "description": "A desktop app for generating Canvas course data",
  "main": "./out/main/index.js",
  "author": "Canvas Capture Team",
  "homepage": "",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:demo": "cross-env RENDERER_VITE_MODE=demo electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "build:multi": "electron-vite build && electron-builder -wl --config",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@electron/remote": "^2.1.2",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.17.19",
    "@tanstack/react-query-devtools": "^5.17.21",
    "@tanstack/react-router": "^1.4.10",
    "@types/lodash": "^4.14.202",
    "antd": "^5.13.1",
    "axios": "^1.6.5",
    "cross-env": "^7.0.3",
    "date-fns": "^3.3.1",
    "electron-updater": "^6.1.7",
    "lodash": "^4.17.21",
    "vite-plugin-electron": "^0.15.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tanstack/eslint-plugin-query": "^5.17.7",
    "@types/node": "^18.19.5",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "electron": "28.1.1",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^1.2.1"
  }
}
